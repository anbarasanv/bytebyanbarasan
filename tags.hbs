{{!< default}}

  <main class="gh-main gh-outer">
    <div class="gh-feed gh-inner">
      {{#get 'tags' limit='all' include='count.posts' order='count.posts desc'}}
      {{#foreach tags}}
          <a href='{{ url }}'>
          <h2>{{ name }} <small>({{ count.posts }})</small></h2>
          <img src="{{feature_image}}" alt="Tag Image">
          </a>
      {{/foreach}}
      {{/get}}
    </div>
  </main>